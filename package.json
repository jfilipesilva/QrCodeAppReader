{
  "name": "QrCodeReaderApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "test": "jest",
    "postinstall": "patch-package",
    "clean": "yarn install && del-cli ios/build ios/Pods && cd ios && bundle install && bundle exec pod install && cd .. && del-cli android/build android/app/build android/.idea android/.gradle && cd android && ./gradlew clean && cd .."
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.1",
    "@react-native-async-storage/async-storage": "^1.22.3",
    "@react-navigation/material-bottom-tabs": "^6.2.27",
    "@react-navigation/native": "^6.1.7",
    "@reduxjs/toolkit": "^1.9.5",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-paper": "^5.8.0",
    "react-native-reanimated": "^3.7.2",
    "react-native-safe-area-context": "^4.6.3",
    "react-native-screens": "^3.29.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-vision-camera": "^3.9.1",
    "react-redux": "^8.1.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.20.0",
    "@jest/globals": "^29.5.0",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "@react-native/babel-preset": "0.73.21",
    "patch-package": "^7.0.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-navigation|@gorhom|react-native-vector-icons)/)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
